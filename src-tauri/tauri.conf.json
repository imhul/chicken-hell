{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "chicken-hell",
    "version": "0.8.10",
    "identifier": "com.chicken-hell.app",
    "build": {
        "beforeDevCommand": "yarn dev",
        "devUrl": "http://localhost:1420",
        "beforeBuildCommand": "yarn build",
        "frontendDist": "../dist"
    },
    "app": {
        "withGlobalTauri": false,
        "windows": [
            {
                "title": "Chicken Hell v0.8.10",
                "width": 1024,
                "height": 768
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    }
}